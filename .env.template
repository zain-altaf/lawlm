# Required: API key for fetching legal documents from CourtListener
CASELAW_API_KEY=your_courtlistener_api_key_here

# Option 1: If using Qdrant Cloud (Recommended for Production)
# Get your cluster URL and API key from: https://cloud.qdrant.io/
# Free tier provides 1GB storage permanently
QDRANT_URL=https://your-cluster-id.us-east-1-0.aws.cloud.qdrant.io:6333
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_CLUSTER_NAME=your_cluster_name

# Option 2: If using local Qdrant instance (Recommended for Development)
# `bash ./manage_qdrant.sh start` to start local instance

# if you are using legal_rag_query.py
OPENAI_API_KEY=your_openai_api_key_here